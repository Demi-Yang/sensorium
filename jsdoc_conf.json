{
    "opts": {
        "template": "./node_modules/docdash",
        "encoding": "utf8",
        "destination": "../sensorium-doc/html/api",
        "recurse": true
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": [ "./src" ],
        "exclude": ["./webpack.config.js", "cli.js"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "includeDate": true,
            "layoutFile": "./layout.tmpl"
        }
    }
}