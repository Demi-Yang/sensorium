{   
    "opts": {
        "template": "./node_modules/docdash",
        "encoding": "utf8",
        "destination": "./jsdoc/",
        "recurse": true
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": [ "./src" ],
        "exclude": ["./webpack.config.js", "cli.js"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}